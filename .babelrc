{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "current",
        "browsers": "> 0.25%, not dead"
      },
      "modules": "umd"
    }]
  ],
  "plugins": [
    ["@babel/plugin-transform-modules-umd", {
      "globals": {
        "swiss-ssn": "SwissSSN"
      }
    }]
  ]
}
